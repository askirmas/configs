{
  "env": {
    "amd": true,
    "browser": true,
    "es6": true,
    "jest": true,
    "node": true
  },
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended",
    "plugin:@typescript-eslint/eslint-recommended",
    "plugin:@typescript-eslint/recommended",
    "plugin:@typescript-eslint/recommended-requiring-type-checking",
    "plugin:unicorn/recommended",
    "plugin:import/errors",
    "plugin:import/warnings",
    "plugin:@mysticatea/es2016",
    "plugin:@mysticatea/+browser",
    "plugin:@mysticatea/+modules",
    "plugin:@mysticatea/+node",
    "plugin:sonarjs/recommended",
    "plugin:no-use-extend-native/recommended"
  ],
  "globals": {
    "__dirname": true,
    "module": true,
    "require": true
  },
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "project": "./tsconfig.eslint.json"
  },
  "plugins": [
    "@typescript-eslint",
    "react",
    "unicorn",
    "import",
    "optimize-regex",
    "sonarjs",
    "no-use-extend-native"
  ],
  "root": true,
  "rules": {
    "@mysticatea/eslint-comments/disable-enable-pair": "off",
    "@mysticatea/eslint-comments/no-unused-disable": "off",
    "@mysticatea/no-use-ignored-vars": "off",
    "@mysticatea/node/no-unpublished-import": "off",
    "@mysticatea/node/no-unpublished-require": "off",
    "@mysticatea/prefer-for-of": 0,
    "@mysticatea/prettier": "off",
    "@mysticatea/ts/array-type": "off",
    "@mysticatea/ts/ban-ts-ignore": 0,
    "@mysticatea/ts/camelcase": "off",
    "@mysticatea/ts/class-name-casing": "off",
    "@mysticatea/ts/explicit-member-accessibility": "off",
    "@mysticatea/ts/no-require-imports": "off",
    "@mysticatea/ts/no-var-requires": "off",
    "@mysticatea/ts/prefer-regexp-exec": "off",
    "@mysticatea/ts/prefer-string-starts-ends-with": "off",
    "@mysticatea/ts/require-await": "off",
    "@typescript-eslint/ban-ts-comment": 0,
    "@typescript-eslint/ban-ts-ignore": 0,
    "@typescript-eslint/explicit-function-return-type": 0,
    "@typescript-eslint/explicit-module-boundary-types": 0,
    "@typescript-eslint/member-delimiter-style": [
      1,
      {
        "multiline": {
          "delimiter": "none",
          "requireLast": false
        }
      }
    ],
    "@typescript-eslint/no-non-null-assertion": 2,
    "@typescript-eslint/no-unnecessary-type-assertion": 2,
    "@typescript-eslint/no-unsafe-assignment": "off",
    "@typescript-eslint/no-unsafe-call": "off",
    "@typescript-eslint/no-unsafe-member-access": "off",
    "@typescript-eslint/no-unsafe-return": "off",
    "@typescript-eslint/no-var-requires": "off",
    "@typescript-eslint/prefer-regexp-exec": "off",
    "@typescript-eslint/require-await": "off",
    "@typescript-eslint/restrict-template-expressions": "off",
    "@typescript-eslint/type-annotation-spacing": [
      2,
      {
        "after": true,
        "before": false,
        "overrides": {
          "arrow": {
            "after": true,
            "before": true
          },
          "returnType": {
            "after": false,
            "before": true
          }
        }
      }
    ],
    "accessor-pairs": 2,
    "array-bracket-newline": "off",
    "array-bracket-spacing": 2,
    "array-callback-return": 2,
    "array-element-newline": "off",
    "arrow-body-style": [
      2,
      "as-needed",
      {
        "requireReturnForObjectLiteral": false
      }
    ],
    "arrow-parens": [
      2,
      "as-needed",
      {
        "requireForBlockBody": false
      }
    ],
    "arrow-spacing": 2,
    "block-scoped-var": 2,
    "block-spacing": 2,
    "brace-style": [
      2,
      "1tbs"
    ],
    "camelcase": 0,
    "capitalized-comments": 0,
    "class-methods-use-this": 2,
    "comma-dangle": 2,
    "comma-spacing": 2,
    "comma-style": [
      2,
      "last",
      {
        "exceptions": {
          "VariableDeclaration": true
        }
      }
    ],
    "complexity": [
      2,
      44
    ],
    "computed-property-spacing": 2,
    "consistent-return": "warn",
    "consistent-this": 2,
    "constructor-super": 2,
    "curly": [
      2,
      "multi"
    ],
    "default-case": "off",
    "default-case-last": 2,
    "default-param-last": "warn",
    "dot-location": [
      2,
      "property"
    ],
    "dot-notation": 2,
    "eol-last": 2,
    "eqeqeq": 2,
    "for-direction": 2,
    "func-call-spacing": 2,
    "func-name-matching": 2,
    "func-names": 2,
    "func-style": [
      2,
      "declaration",
      {
        "allowArrowFunctions": true
      }
    ],
    "function-call-argument-newline": 0,
    "function-paren-newline": 0,
    "generator-star-spacing": [
      2,
      "after"
    ],
    "getter-return": 2,
    "grouped-accessor-pairs": 2,
    "guard-for-in": 0,
    "id-blacklist": 2,
    "id-length": [
      2,
      {
        "exceptions": [
          "_",
          "a",
          "__",
          "_l",
          "_r",
          "_x",
          "_y",
          "a1",
          "a2",
          "c",
          "c0",
          "c1",
          "cb",
          "cc",
          "ch",
          "ck",
          "cl",
          "cn",
          "co",
          "cw",
          "d",
          "dc",
          "dm",
          "dx",
          "dy",
          "e",
          "el",
          "en",
          "ev",
          "ex",
          "ey",
          "fn",
          "fs",
          "g",
          "h",
          "i",
          "i1",
          "i2",
          "id",
          "it",
          "ix",
          "iy",
          "j",
          "k",
          "k1",
          "k2",
          "l",
          "mx",
          "my",
          "n",
          "nV",
          "o",
          "p",
          "p1",
          "p2",
          "pr",
          "q",
          "qq",
          "r",
          "rl",
          "rs",
          "s",
          "sV",
          "sc",
          "se",
          "ss",
          "sx",
          "sy",
          "t",
          "th",
          "to",
          "tw",
          "v",
          "v0",
          "v1",
          "v2",
          "vc",
          "vs",
          "w",
          "x",
          "xi",
          "xy",
          "y",
          "yi"
        ],
        "max": 40,
        "min": 3
      }
    ],
    "id-match": 2,
    "implicit-arrow-linebreak": [
      "off",
      "beside"
    ],
    "import/no-unresolved": "off",
    "indent": [
      2,
      2,
      {
        "ignoredNodes": [
          "ConditionalExpression"
        ],
        "MemberExpression": 0,
        "SwitchCase": 1,
        "VariableDeclarator": 0
      }
    ],
    "init-declarations": 0,
    "jsx-quotes": 2,
    "key-spacing": 2,
    "keyword-spacing": 2,
    "line-comment-position": "warn",
    "linebreak-style": 2,
    "lines-around-comment": "off",
    "lines-between-class-members": [
      2,
      "always",
      {
        "exceptAfterSingleLine": true
      }
    ],
    "max-classes-per-file": [
      2,
      3
    ],
    "max-depth": [
      2,
      6
    ],
    "max-len": [
      2,
      120,
      {
        "ignoreComments": true
      }
    ],
    "max-lines": [
      2,
      577
    ],
    "max-lines-per-function": [
      2,
      320
    ],
    "max-nested-callbacks": [
      2,
      3
    ],
    "max-params": [
      2,
      6
    ],
    "max-statements": [
      2,
      52
    ],
    "max-statements-per-line": 2,
    "multiline-comment-style": "off",
    "multiline-ternary": [
      2,
      "always-multiline"
    ],
    "new-cap": 2,
    "new-parens": 2,
    "newline-after-var": 2,
    "newline-before-return": 2,
    "newline-per-chained-call": 2,
    "no-alert": 2,
    "no-array-constructor": 2,
    "no-async-promise-executor": 2,
    "no-await-in-loop": 2,
    "no-bitwise": "warn",
    "no-caller": 2,
    "no-case-declarations": 2,
    "no-class-assign": 2,
    "no-compare-neg-zero": 2,
    "no-cond-assign": 2,
    "no-confusing-arrow": 0,
    "no-console": 2,
    "no-const-assign": 2,
    "no-constant-condition": "warn",
    "no-constructor-return": 2,
    "no-continue": "warn",
    "no-control-regex": 2,
    "no-debugger": 2,
    "no-delete-var": 2,
    "no-div-regex": 2,
    "no-dupe-args": 2,
    "no-dupe-class-members": 2,
    "no-dupe-else-if": 2,
    "no-dupe-keys": 2,
    "no-duplicate-case": 2,
    "no-duplicate-imports": 2,
    "no-else-return": 2,
    "no-empty": 2,
    "no-empty-character-class": 2,
    "no-empty-function": 2,
    "no-empty-pattern": 2,
    "no-eq-null": 2,
    "no-eval": 2,
    "no-ex-assign": 2,
    "no-extend-native": 2,
    "no-extra-bind": 2,
    "no-extra-boolean-cast": 2,
    "no-extra-label": 2,
    "no-extra-parens": 2,
    "no-extra-semi": 2,
    "no-fallthrough": 2,
    "no-floating-decimal": 2,
    "no-func-assign": 2,
    "no-global-assign": 2,
    "no-implicit-coercion": [
      2,
      {
        "number": false
      }
    ],
    "no-implicit-globals": 2,
    "no-implied-eval": 2,
    "no-import-assign": 2,
    "no-inline-comments": "warn",
    "no-inner-declarations": 2,
    "no-invalid-regexp": 2,
    "no-invalid-this": "warn",
    "no-irregular-whitespace": 2,
    "no-iterator": 2,
    "no-label-var": 2,
    "no-labels": "off",
    "no-lone-blocks": 2,
    "no-lonely-if": 2,
    "no-loop-func": 2,
    "no-loss-of-precision": 2,
    "no-magic-numbers": "off",
    "no-misleading-character-class": 2,
    "no-mixed-operators": 0,
    "no-mixed-spaces-and-tabs": 2,
    "no-multi-assign": 2,
    "no-multi-spaces": 2,
    "no-multi-str": 2,
    "no-multiple-empty-lines": [
      2,
      {
        "max": 1,
        "maxBOF": 0
      }
    ],
    "no-native-reassign": 2,
    "no-negated-condition": "off",
    "no-nested-ternary": 0,
    "no-new": 2,
    "no-new-func": 2,
    "no-new-object": 2,
    "no-new-require": 2,
    "no-new-symbol": 2,
    "no-new-wrappers": 2,
    "no-obj-calls": 2,
    "no-octal": 2,
    "no-octal-escape": 2,
    "no-param-reassign": 2,
    "no-plusplus": 0,
    "no-process-exit": "off",
    "no-proto": 2,
    "no-prototype-builtins": 2,
    "no-redeclare": 2,
    "no-regex-spaces": 2,
    "no-restricted-exports": 2,
    "no-restricted-globals": 2,
    "no-restricted-imports": 2,
    "no-restricted-properties": 2,
    "no-restricted-syntax": 2,
    "no-return-assign": "warn",
    "no-return-await": 2,
    "no-script-url": 2,
    "no-self-assign": 2,
    "no-self-compare": 2,
    "no-sequences": "warn",
    "no-setter-return": 2,
    "no-shadow": "warn",
    "no-shadow-restricted-names": 2,
    "no-spaced-func": 2,
    "no-sparse-arrays": 2,
    "no-tabs": 2,
    "no-template-curly-in-string": 2,
    "no-ternary": 0,
    "no-this-before-super": 2,
    "no-throw-literal": 2,
    "no-trailing-spaces": 2,
    "no-undef": 2,
    "no-undef-init": 0,
    "no-undefined": 0,
    "no-underscore-dangle": "warn",
    "no-unexpected-multiline": 2,
    "no-unmodified-loop-condition": "warn",
    "no-unneeded-ternary": 2,
    "no-unreachable": 2,
    "no-unsafe-finally": 2,
    "no-unsafe-negation": 2,
    "no-unused-expressions": "off",
    "no-unused-labels": 2,
    "no-unused-vars": [
      "warn",
      {
        "ignoreRestSiblings": true,
        "varsIgnorePattern": "_"
      }
    ],
    "no-use-before-define": [
      2,
      {
        "functions": false
      }
    ],
    "no-useless-backreference": 2,
    "no-useless-call": 2,
    "no-useless-catch": 2,
    "no-useless-computed-key": 2,
    "no-useless-concat": 2,
    "no-useless-constructor": 2,
    "no-useless-escape": 2,
    "no-useless-rename": 2,
    "no-useless-return": 0,
    "no-var": 2,
    "no-void": 2,
    "no-warning-comments": "off",
    "no-whitespace-before-property": 2,
    "no-with": 2,
    "nonblock-statement-body-position": [
      2,
      "below"
    ],
    "object-curly-newline": [
      2,
      {
        "ExportDeclaration": "always",
        "ImportDeclaration": {
          "minProperties": 3,
          "multiline": true
        },
        "ObjectExpression": {
          "minProperties": 3,
          "multiline": true
        },
        "ObjectPattern": {
          "minProperties": 3,
          "multiline": true
        }
      }
    ],
    "object-curly-spacing": [
      2,
      "never"
    ],
    "object-property-newline": "off",
    "object-shorthand": "off",
    "one-var": [
      2,
      "consecutive"
    ],
    "one-var-declaration-per-line": 2,
    "operator-assignment": 2,
    "operator-linebreak": [
      2,
      "before",
      {}
    ],
    "optimize-regex/optimize-regex": "off",
    "padded-blocks": "off",
    "padding-line-between-statements": 2,
    "prefer-arrow-callback": 2,
    "prefer-const": 2,
    "prefer-destructuring": [
      2,
      {
        "array": false,
        "object": true
      },
      {
        "enforceForRenamedProperties": false
      }
    ],
    "prefer-exponentiation-operator": 2,
    "prefer-named-capture-group": 0,
    "prefer-numeric-literals": 2,
    "prefer-object-spread": 2,
    "prefer-promise-reject-errors": 2,
    "prefer-regex-literals": 2,
    "prefer-rest-params": "warn",
    "prefer-spread": 2,
    "prefer-template": 2,
    "quote-props": [
      2,
      "consistent"
    ],
    "quotes": 0,
    "radix": 2,
    "react/button-has-type": "warn",
    "react/destructuring-assignment": "warn",
    "react/forbid-component-props": "warn",
    "react/function-component-definition": "error",
    "react/jsx-closing-bracket-location": "off",
    "react/jsx-closing-tag-location": "off",
    "react/jsx-curly-brace-presence": [
      "off",
      {
        "children": "never",
        "props": "never"
      }
    ],
    "react/jsx-filename-extension": [
      2,
      {
        "extensions": [
          ".jsx",
          ".tsx"
        ]
      }
    ],
    "react/jsx-first-prop-new-line": [
      "off",
      "never"
    ],
    "react/jsx-indent": "off",
    "react/jsx-indent-props": [
      2,
      2
    ],
    "react/jsx-key": 2,
    "react/jsx-max-depth": [
      2,
      {
        "max": 3
      }
    ],
    "react/jsx-max-props-per-line": [
      2,
      {
        "when": "multiline"
      }
    ],
    "react/jsx-no-bind": 2,
    "react/jsx-no-duplicate-props": 2,
    "react/jsx-no-literals": [
      "warn",
      {
        "ignoreProps": true,
        "noStrings": true
      }
    ],
    "react/jsx-no-undef": 2,
    "react/jsx-no-useless-fragment": "off",
    "react/jsx-one-expression-per-line": 0,
    "react/jsx-props-no-multi-spaces": 2,
    "react/jsx-props-no-spreading": 0,
    "react/jsx-sort-props": "off",
    "react/jsx-tag-spacing": [
      2,
      {
        "afterOpening": "never",
        "beforeClosing": "never",
        "beforeSelfClosing": "never",
        "closingSlash": "never"
      }
    ],
    "react/jsx-wrap-multilines": "off",
    "react/no-access-state-in-setstate": 2,
    "react/no-array-index-key": "warn",
    "react/no-multi-comp": "off",
    "react/no-set-state": 0,
    "react/no-unsafe": 0,
    "react/no-unused-state": 2,
    "react/prefer-stateless-function": 2,
    "react/prop-types": 0,
    "react/react-in-jsx-scope": 0,
    "react/self-closing-comp": 2,
    "react/sort-comp": "off",
    "react/state-in-constructor": [
      2,
      "never"
    ],
    "react/style-prop-object": 2,
    "require-atomic-updates": 2,
    "require-await": "warn",
    "require-unicode-regexp": "warn",
    "require-yield": 2,
    "rest-spread-spacing": 2,
    "semi": [
      2,
      "never"
    ],
    "semi-spacing": 2,
    "semi-style": 2,
    "sonarjs/cognitive-complexity": "off",
    "sonarjs/no-duplicate-string": [
      2,
      3
    ],
    "sonarjs/no-identical-functions": "off",
    "sonarjs/no-redundant-jump": "off",
    "sort-imports": [
      2,
      {
        "ignoreDeclarationSort": true
      }
    ],
    "sort-keys": "warn",
    "sort-vars": "off",
    "space-before-blocks": 2,
    "space-before-function-paren": [
      2,
      "never"
    ],
    "space-in-parens": 2,
    "space-infix-ops": 2,
    "space-unary-ops": 2,
    "spaced-comment": "off",
    "strict": 2,
    "switch-colon-spacing": 2,
    "symbol-description": 2,
    "template-curly-spacing": 2,
    "template-tag-spacing": 2,
    "unicode-bom": 2,
    "unicorn/better-regex": "off",
    "unicorn/explicit-length-check": "off",
    "unicorn/filename-case": "off",
    "unicorn/no-fn-reference-in-iterator": "off",
    "unicorn/no-for-loop": 0,
    "unicorn/no-nested-ternary": "off",
    "unicorn/no-null": "off",
    "unicorn/no-process-exit": "off",
    "unicorn/no-reduce": "off",
    "unicorn/no-unreadable-array-destructuring": "off",
    "unicorn/no-useless-undefined": "off",
    "unicorn/prefer-flat-map": "off",
    "unicorn/prefer-number-properties": "off",
    "unicorn/prefer-set-has": 2,
    "unicorn/prevent-abbreviations": "off",
    "use-isnan": 2,
    "valid-typeof": 2,
    "vars-on-top": 2,
    "wrap-iife": 2,
    "wrap-regex": 2,
    "yield-star-spacing": 2,
    "yoda": 0
  },
  "settings": {
    "linkComponents": [
      {
        "linkAttribute": "href",
        "name": "a"
      },
      "Hyperlink",
      {
        "linkAttribute": "to",
        "name": "Link"
      }
    ],
    "propWrapperFunctions": [
      "forbidExtraProps",
      "exact",
      "Object.freeze"
    ],
    "react": {
      "version": "detect"
    }
  }
}